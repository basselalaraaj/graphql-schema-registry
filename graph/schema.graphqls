type Query {
  getSchemas(services: [String!]!): [Schema!]!
}

type Mutation {
  pushSchema(schemaInput: SchemaInput!): Boolean!
}

input SchemaInput {
  serviceName: String!
  serviceUrl: String!
  typeDefs: String!
}

type Schema {
  serviceName: String!
  serviceUrl: String!
  typeDefs: String!
}
